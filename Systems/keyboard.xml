<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<key n="2">
		<name>Ctrl-B</name>
		<desc>Toggle Speedbrake</desc>
		<binding>
			<command>property-toggle</command>
			<property>controls/flight/speedbrake</property>
		</binding>
	</key>
	<key n="60">
		<name>&lt;</name>
		<desc>Decrease Sweep</desc>
		<binding>
			<command>nasal</command>
			<script>libraries.wingSweep(-1)</script>
		</binding>
	</key>
	<key n="62">
		<name>&gt;</name>
		<desc>Increase Sweep</desc>
		<binding>
			<command>nasal</command>
			<script>libraries.wingSweep(1)</script>
		</binding>
	</key>
	<key n="68">
		<name>D</name>
		<desc>Close Canopy</desc>
		<binding>
			<command>property-assign</command>
			<property>sim/model/canopy-positions/Canopy/position-norm</property>
			<value>0</value>
		</binding>
	</key>
	<key n="100">
		<name>d</name>
		<desc>Open Canopy</desc>
		<binding>
			<command>property-assign</command>
			<property>sim/model/canopy-positions/Canopy/position-norm</property>
			<value>1</value>
		</binding>
	</key>
	<key n="76">
		<name>L</name>
		<desc>Cockpit Light</desc>
		<binding>
			<command>property-toggle</command>
			<property>instrumentation/cockpit_lights/Connected</property>
		</binding>
	</key>
	<key n="108">
		<name>l</name>
		<desc>Landing Light</desc>
		<binding>
			<command>property-toggle</command>
			<property>instrumentation/landing_lights/Connected</property>
		</binding>
	</key>
	<key n="127">
		<name>Delete</name>
		<desc>Toggle Reversers</desc>
		<binding>
			<command>nasal</command>
			<script>libraries.togglereverser()</script>
		</binding>
	</key>
</PropertyList>

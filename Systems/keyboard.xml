<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<key n="2">
		<name>Ctrl-B</name>
		<desc>Toggle speedbrake</desc>
		<binding>
			<command>property-toggle</command>
			<property>controls/flight/speedbrake</property>
		</binding>
	</key>
	<key n="60">
		<name>&lt;</name>
		<desc>Decrease Sweep</desc>
		<binding>
			<command>nasal</command>
			<script>libraries.wingSweep(-1)</script>
		</binding>
	</key>
	<key n="62">
		<name>&gt;</name>
		<desc>Increase Sweep</desc>
		<binding>
			<command>nasal</command>
			<script>libraries.wingSweep(1)</script>
		</binding>
	</key>
	<key n="68">
		<name>D</name>
		<desc>close-canopy</desc>
		<binding>
			<command>property-assign</command>
			<property>sim/model/canopy-positions/Canopy/position-norm</property>
			<value>0</value>
		</binding>
	</key>
	<key n="100">
		<name>d</name>
		<desc>canopy</desc>
		<binding>
			<command>property-assign</command>
			<property>sim/model/canopy-positions/Canopy/position-norm</property>
			<value>1</value>
		</binding>
	</key>
	<key n="127">
		<name>Delete</name>
		<desc>Toggle Reversers</desc>
		<binding>
			<command>nasal</command>
			<script>libraries.togglereverser()</script>
		</binding>
	</key>
</PropertyList>

<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<view n="100">
		<name>Navigators View</name>
		<internal archive="y">true</internal>
		<type>lookfrom</type>
		<config>
			<from-model type="bool">true</from-model>
			<ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
			<pitch-offset-deg>-12.0</pitch-offset-deg>
			<x-offset-m archive="y">0.0</x-offset-m>
			<y-offset-m archive="y">-0.1</y-offset-m>
			<z-offset-m archive="y">-2.5</z-offset-m>
			<default-field-of-view-deg>80</default-field-of-view-deg>
			<dynamic-view type="bool">true</dynamic-view>
		</config>
	</view>
	<view n="0">
		<name>Pilots View</name>
		<internal archive="y">true</internal>

<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>cover1.ac</path>
	<animation>
		<object-name>cover</object-name>
		<type>rotate</type>
		<property>instrumentation/buttons/cover2</property>
		<factor>-90</factor>
		<center>
			<x-m>0.000</x-m>
			<y-m>0.000</y-m>
			<z-m>0.006</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>cover</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>instrumentation/buttons/cover2</property>
			</binding>
		</action>
	</animation>
</PropertyList>

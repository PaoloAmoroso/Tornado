<?xml version="1.0"?>
<PropertyList>
	<path>rpm.ac</path>
	<effect>
		<inherits-from>Aircraft/Tornado/Models/Effects/instruments/lm1</inherits-from>
		<object-name>BG</object-name>
		<object-name>needle</object-name>
		<object-name>Plane.001</object-name>
	</effect>
	<animation>
		<type>select</type>
		<object-name>Plane</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/ElBus/Voltage</property>
				<value>24</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<name>needle</name>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>engines/engine[1]/n1</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>-125.0</dep></entry>
			<entry><ind>120.0</ind><dep>125.0</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
</PropertyList>

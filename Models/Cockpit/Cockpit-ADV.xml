<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<model>
		<name>Cockpit-Common</name>
		<path>Aircraft/Tornado/Models/Cockpit/Cockpit-Common.xml</path>
	</model>
	<model>
		<name>Pilots Weapon Control Panel</name>
		<path>Aircraft/Tornado/Models/Cockpit/Instruments/pwcp/pwcp.ac</path>
		<offsets>
			<x-m>-4.755</x-m>
			<y-m>-0.233</y-m>
			<z-m>-0.443</z-m>
			<pitch-deg>-10.0</pitch-deg>
		</offsets>
	</model>
	<model>
		<name>Electronic Head Down Display</name>
		<path>Aircraft/Tornado/Models/Cockpit/Instruments/ehdd/ehdd.ac</path>
		<offsets>
			<x-m>-4.730</x-m>
			<y-m>0.000</y-m>
			<z-m>-0.600</z-m>
			<pitch-deg>-25.0</pitch-deg>
		</offsets>
	</model>
	<model>
		<name>TVTAB</name>
		<path>Aircraft/Tornado/Models/Cockpit/Instruments/tvtab/tvtab1.xml</path>
		<offsets>
			<x-m>-4.710</x-m>
			<y-m>0.000</y-m>
			<z-m>-0.650</z-m>
			<pitch-deg>-10.0</pitch-deg>
		</offsets>
	</model>
	<animation>
		<object-name>TVTAB</object-name>
		<type>select</type>
	    <condition>
			<property>instrumentation/tvtab/visible</property>
		</condition>
    </animation>
</PropertyList>

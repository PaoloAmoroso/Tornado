<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<title>Panavia Tornado</title>
	<key>
		<name>b</name>
		<desc>Apply All Brakes</desc>
	</key>
	<key>
		<name>B</name>
		<desc>Tggle Parking Brake</desc>
	</key>
	<key>
		<name>Ctrl+b</name>
		<desc>Toggle Speedbrake</desc>
	</key>
	<key>
		<name>d</name>
		<desc>Open/Close Canopy</desc>
	</key>
	<key>
		<name>g/G</name>
		<desc>Gear Up/Down</desc>
	</key>
	<key>
		<name>f</name>
		<desc>Refuelling Probe</desc>
	</key>
	<key>
		<name>l</name>
		<desc>Landing Lights</desc>
	</key>
	<key>
		<name>L</name>
		<desc>Cockpit Lights</desc>
	</key>
	<key>
		<name>Ctrl-v</name>
		<desc>Back to Pilot's View or toggle Pilot's and Navigator's View</desc>
	</key>
	<key>
		<name>&gt;</name>
		<desc>Wing Sweep Aft</desc>
	</key>
	<key>
		<name>&lt;</name>
		<desc>Wing Sweep Forward</desc>
	</key>
	<key>
		<name>[ / ]</name>
		<desc>Flaps Up/Down</desc>
	</key>
	<key>
		<name>Del</name>
		<desc>Thrust Reverser</desc>
	</key>
	<key>
		<name>Q</name>
		<desc>Reset View</desc>
	</key>
</PropertyList>
